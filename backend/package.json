    "openapi-fetch": "^0.13.5",
    "postgres": "^3.4.5"
  },
  "devDependencies": {
    "@vercel/node": "^5.1.14",
    "drizzle-kit": "^0.31.0",
    "esbuild": "^0.25.3",
    "tsx": "^4.19.3"
