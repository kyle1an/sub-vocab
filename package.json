{
  "name": "sub-vocab",
  "type": "module",
  "version": "0.3.0",
  "private": true,
  "packageManager": "pnpm@10.9.0",
  "scripts": {
    "backend": "pnpm --filter @sub-vocab/backend",
    "bump": "taze major -r && pnpm lint:pkg && pnpm i && pnpm up && npx nolyfill",
    "introspect": "pnpm --recursive run introspect",
    "lint:fix": "pnpm --recursive run lint:fix",
    "lint:pkg": "eslint --fix package.json **/package.json",
    "ui": "pnpm --filter @sub-vocab/ui"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.4",
    "date-fns": "^4.1.0",
    "neverthrow": "^8.2.0",
    "type-fest": "^4.40.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "@eslint/eslintrc": "^3.3.1",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@t3-oss/env-core": "^0.13.0",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/node": "^22.15.2",
    "eslint": "^9.25.1",
    "eslint-plugin-command": "^3.2.0",
    "eslint-plugin-de-morgan": "^1.2.1",
    "eslint-plugin-jsonc": "^2.20.0",
    "eslint-plugin-package-json": "^0.30.0",
    "jsonc-eslint-parser": "^2.4.0",
    "openapi-typescript": "^7.6.1",
    "pathe": "^2.0.3",
    "prettier": "^3.5.3",
    "taze": "^19.0.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "pnpm": {
    "overrides": {
      "@vercel/node>ts-node": "$ts-node",
      "@vercel/node>typescript": "$typescript",
      "array-flatten": "npm:@nolyfill/array-flatten@^1.0.44",
      "es-set-tostringtag": "npm:@nolyfill/es-set-tostringtag@^1.0.44",
      "hasown": "npm:@nolyfill/hasown@^1.0.44",
      "is-core-module": "npm:@nolyfill/is-core-module@^1.0.39",
      "safe-buffer": "npm:@nolyfill/safe-buffer@^1.0.44",
      "safer-buffer": "npm:@nolyfill/safer-buffer@^1.0.44",
      "side-channel": "npm:@nolyfill/side-channel@^1.0.44",
      "whatwg-url": "^14.2.0"
    },
    "onlyBuiltDependencies": [
      "@vercel/speed-insights",
      "esbuild",
      "supabase",
      "unrs-resolver"
    ]
  },
  "resolutions": {
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc"
  }
}
